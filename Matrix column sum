#include<stdio.h>
int main(){
	int m,n,a[10][10],sum,i,j;
	printf("Enter the no. of rows & columns: ");
	scanf("%d %d",&m, &n);
	printf("Enter the elements: \n");
	for(i=0;i<m;i++){
		for(j=0;j<n;j++);{
			scanf("%d ",&a[i][j]);
		}
	}
	for (i=0;i<m;i++){
		sum=0;
		for(int j=0;j<n;j++){
			sum+=a[j][i];
		}
		printf("Sum of Column %d= %d \n",i+1,sum);
    }
	return 0;
}
